<Alloy>
	<View class="container" height="42" hasChild="getSharedWithHerSubProjects():project/projectShareRow">
		<View id="content" class="row-content" height="Ti.UI.FILL">
			<View layout="horizontal" horizontalWrap="false" left="10" width="Ti.UI.SIZE">
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" left="10" top="0" width="Ti.UI.SIZE" height="42"
				bindModel = "$.$model.project"
				bindAttribute = "name"
				/>
			</View>
			<Label id="checkAccept" visible="false" color="black" right="15">等待接受</Label>
		</View>
	</View>
</Alloy>