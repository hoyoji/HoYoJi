<Alloy>
	<View class="container" height="42" openForm="project/projectForm" hasChild="getSharedWithHerSubProjects():project/projectShareRow">
			<View id="content" class="row-content" left="0" height="Ti.UI.FILL" backgroundColor="#f5f5f5">
			<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" left="10" top="0" width="Ti.UI.SIZE" height="42"
				id="name" autoInit="false"
				bindModel = "$.$model.project"
				bindAttribute = "name"
				/>
				<Label id="checkAccept" visible="false" color="black" right="15">等待接受</Label>
			</View>
	</View>
</Alloy>