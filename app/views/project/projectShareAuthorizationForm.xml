					<Widget src="com.hoyoji.titanium.widget.AutoUpdatableTextField" labelText="项目" editModeEditability="noneditable"
                		bindModel="$.$model"
                		bindAttribute="project"
                		bindAttributeIsModel="name"
                		/>
					<Widget src="com.hoyoji.titanium.widget.AutoUpdatableTextField" labelText="好友" hintText="请选择好友"
						bindModel="$.$model"
                        bindAttribute="friend"
						bindAttributeIsModel="getDisplayName()"
						bindModelSelector="friend/friendAll"
					/>
