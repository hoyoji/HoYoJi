<Alloy>
	<View class="container" height="84" openForm="money/moneyReturnForm">
		<View width="Ti.UI.FILL" height="1" top="0" left="10" right="10" backgroundImage="/images/rowTopShadow.png" backgroundImageRepeat="true"/>
		<!-- <View width="3" height="Ti.UI.FILL" top="0" left="0" backgroundImage="/images/rowLeftShadow.png" backgroundImageRepeat="true"/> -->
			<View id="content" class="row-content" left="3" height="Ti.UI.FILL" backgroundColor="#f8f8f8">
			<!-- <View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="0" left="5" width="Ti.UI.SIZE" height="42" class="firstRow"
				bindModel = "$.$model"
				bindAttribute = "getProjectName()"
				/>
			<!-- </View>
			<View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="0" right="5" width="Ti.UI.SIZE" height="42" class="firstRow"
				dataType="DateTime"
				bindModel = "$.$model"
				bindAttribute = "date"
				/>
			<!-- </View>
			<View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false"> -->
				<Label top="42" left="5" width="Ti.UI.SIZE" height="42" class="secondRow">还款支付了利息</Label>
			<!-- </View>
			<View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="42" right="5" width="Ti.UI.SIZE" height="42"
				bindModel = "$.$model"
				bindAttribute = "getInterest()"
				/>
			<!-- </View> -->
		</View>
	</View>
</Alloy>