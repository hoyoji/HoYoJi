<Alloy>
	<View class="container" height="42">
		<View id="content" class="row-content" height="Ti.UI.FILL" backgroundColor="#f5f5f5">
			<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" left="10"  width="Ti.UI.SIZE" height="42"
			id="name" autoInit="false"
			bindModel = "$.$model"
			bindAttribute = "name"
			/>
			<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="0"  width="Ti.UI.SIZE" height="21" right="20"
			id="code" autoInit="false"
			bindModel = "$.$model"
			bindAttribute = "code"
			/>
			<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="21"  width="Ti.UI.SIZE" height="21" right="20" class="secondRow"
			id="symbol" autoInit="false"
			bindModel = "$.$model"
			bindAttribute = "symbol"
			/>
		</View>
	</View>
</Alloy>