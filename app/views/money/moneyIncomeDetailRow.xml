			<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" left="10" top="0" width="Ti.UI.SIZE" height="42"
			bindModel = "$.$model"
			bindAttribute = "name"
			/>
			<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" right="10" top="0" width="Ti.UI.SIZE" height="42"
			bindModel="$.$model"
			bindAttribute="amount"
			/>
		</View>
	</View>
</Alloy>