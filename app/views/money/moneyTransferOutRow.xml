<Alloy>
	<View class="container" height="90" openForm="money/moneyTransferForm">
		<View width="Ti.UI.FILL" height="1" top="0" left="10" right="10" backgroundImage="/images/rowTopShadow.png" backgroundImageRepeat="true"/>
		<!-- <View width="3" height="Ti.UI.FILL" top="0" left="0" backgroundImage="/images/rowLeftShadow.png" backgroundImageRepeat="true"/> -->
			<View id="content" class="row-content" left="3" height="Ti.UI.FILL" backgroundImage="/images/rowBackgroundColor.png">
			<!-- <View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="0" left="5" width="Ti.UI.SIZE" height="30" class="firstRow"
				bindModel = "$.$model"
				bindAttribute = "getProjectName()"
				/>
				
			<!-- </View>
			<View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="0" right="5" width="Ti.UI.SIZE" height="30" class="firstRow"
				dataType="DateTime"
				bindModel = "$.$model"
				bindAttribute = "date"
				/>
			<!-- </View> -->
			<!-- <View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="30" left="5" width="Ti.UI.SIZE" height="30" class="secondRow"
				bindModel = "$.$model"
				bindAttribute = "getTransferOut()"
				/>
				<!-- <Label width="Ti.UI.SIZE" top="30" height="30" class="secondRow">转出</Label>
			</View> -->
			<!-- <View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="30" right="5" width="Ti.UI.SIZE" height="30" class="secondRow"
				bindModel = "$.$model"
				bindAttribute = "getTransferOutAmount()"
				/>
			<!-- </View>
			<View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="60" left="5" width="Ti.UI.SIZE" height="30"
				bindModel = "$.$model"
				bindAttribute = "remark"
				/>
			<!-- </View> -->
		</View>
	</View>
</Alloy>