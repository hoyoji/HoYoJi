<Alloy>
	<View class="container" height="90" openForm="money/moneyTransferForm">
		<!-- <View width="Ti.UI.FILL" height="1" top="0" left="10" right="10" backgroundImage="/images/rowTopShadow.png" backgroundImageRepeat="true"/> -->
			<View id="content" class="row-content" left="0" height="Ti.UI.FILL" backgroundColor="#f5f5f5">
				<Widget src="com.hoyoji.titanium.widget.AutoBindPicturePreview" width="56" height="56" left="5"
					id="picture" autoInit="false"
					bindModel="$.$model"
					bindAttribute="pictureId"/>
			
						<!-- <View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="16" left="66" width="Ti.UI.SIZE" height="Ti.UI.SIZE" class="firstRow" 
					id="projectName" autoInit="false"
					bindModel = "$.$model"
					bindAttribute = "getProjectName()"
				/>
				
			<!-- </View>
			<View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="16" right="5" width="Ti.UI.SIZE" height="Ti.UI.SIZE" class="firstRow" 
				id="date" autoInit="false"
				dataType="DateTime"
				bindModel = "$.$model"
				bindAttribute = "date"
				/>
			<!-- </View> -->
			<!-- <View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  left="66" width="Ti.UI.SIZE" height="30" class="secondRow"
				id="transferOut" autoInit="false"
				bindModel = "$.$model"
				bindAttribute = "getTransferOut()"
				/>
				<!-- <Label width="Ti.UI.SIZE" top="30" height="30" class="secondRow">转出</Label>
			</View> -->
			<!-- <View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" right="5" height="Ti.UI.SIZE" class="secondRow"
				id="transferOutAmount" autoInit="false"
				bindModel = "$.$model"
				bindAttribute = "getTransferOutAmount()"
				/>
			<!-- </View>
			<View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false"> -->
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="57" left="66" right="30" height="16" class="firstRow" 
				id="remark" autoInit="false"
				bindModel = "$.$model"
				bindAttribute = "getRemark()"
				/>
			<!-- </View> -->
		</View>
	</View>
</Alloy>