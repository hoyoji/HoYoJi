<Alloy>
	<View class="container" height="90" openForm="money/moneyTransferForm">
		<View id="content" class="row-content" height="Ti.UI.FILL">
			<View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false">
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="0" width="Ti.UI.SIZE" height="30"
				bindModel = "$.$model.project"
				bindAttribute = "name"
				/>
				/>
			</View>
			<View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false">
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="0" width="Ti.UI.SIZE" height="30"
				dataType="DateTime"
				bindModel = "$.$model"
				bindAttribute = "date"
				/>
			</View>
			<View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false">
				<Label width="Ti.UI.SIZE" top="30" height="30">转入</Label>
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="30" width="Ti.UI.SIZE" height="30"
				bindModel = "$.$model"
				bindAttribute = "getTransferIn()"
				/>
			</View>
			<View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false">
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="30" width="Ti.UI.SIZE" height="30"
				bindModel = "$.$model.transferIn.currency"
				bindAttribute = "symbol"
				/>
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="30" width="Ti.UI.SIZE" height="30"
				bindModel = "$.$model"
				bindAttribute = "getTransferInAmount()"
				/>
			</View>
			<View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false">
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="60" width="Ti.UI.SIZE" height="30"
				bindModel = "$.$model"
				bindAttribute = "remark"
				/>
			</View>
		</View>
	</View>
</Alloy>