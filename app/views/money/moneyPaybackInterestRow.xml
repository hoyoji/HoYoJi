<Alloy>
	<View class="container" height="84" openForm="money/moneyPaybackForm">
		<View width="3" height="Ti.UI.FILL" top="0" left="0" backgroundImage="/images/rowLeftShadow.png" backgroundImageRepeat="true"/>
			<View id="content" class="row-content" left="3" height="Ti.UI.FILL">
				<View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false">
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel"  top="0" width="Ti.UI.SIZE" height="42" color="#bfbfbf"
				bindModel = "$.$model"
				bindAttribute = "getProjectName()"
				/>
			</View>
			<View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false">
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="0" width="Ti.UI.SIZE" height="42" color="#bfbfbf"
				dataType="DateTime"
				bindModel = "$.$model"
				bindAttribute = "date"
				/>
			</View>
			<View width="Ti.UI.SIZE" height="Ti.UI.FILL" layout="horizontal" left="5" horizontalWrap="false">
				<Label top="42" width="Ti.UI.SIZE" height="42">收款赚了利息</Label>
			</View>
			<View width="Ti.UI.SIZE" right="5" height="Ti.UI.FILL" layout="horizontal" horizontalWrap="false">
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="42" width="Ti.UI.SIZE" height="42"
				bindModel = "$.$model.moneyAccount.currency"
				bindAttribute = "symbol"
				/>
				<Widget src="com.hoyoji.titanium.widget.AutoBindLabel" top="42" width="Ti.UI.SIZE" height="42"
				bindModel = "$.$model"
				bindAttribute = "getInterest()"
				/>
			</View>
		</View>
	</View>
</Alloy>